        "tsup": "^8.4.0",
        "tsx": "^4.19.3",
        "typescript": "^5.8.3"
    },
    "dependencies": {
        "@types/chalk": "^0.4.31",
        "chalk": "^5.4.1",
        "chokidar": "^4.0.3",
        "cors": "^2.8.5",
        "express": "^5.1.0",
        "gray-matter": "^4.0.3",
        "highlight": "^0.2.4",
